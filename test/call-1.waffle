
def and = \(x:Bool, y:Bool) => if x then if y then true else false else false;
def or = \(x:Bool, y:Bool) => if x then true else if y then true else false;

def not = \x:Bool => if x then false else true;

print and;
print and(true, true);
print and(true, false);
print and(false, true);
print and(false, false);

print or;
print or(true, true);
print or(true, false);
print or(false, true);
print or(false, false);

print not;
print not(true);
print not(false);
